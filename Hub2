spawn(function()
    while wait() do
        if _G.Settings.Main["Auto Buy Legendary Sword"] then
            local args = {
                [1] = "LegendarySwordDealer",
                [2] = "1"
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            local args = {
                [1] = "LegendarySwordDealer",
                [2] = "2"
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            local args = {
                [1] = "LegendarySwordDealer",
                [2] = "3"
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    end
end)

