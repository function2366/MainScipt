if not game:IsLoaded() then
    local GameLoadGui = Instance.new("Message", workspace);
    GameLoadGui.Text = 'Wait Game Loading';
    game.Loaded:Wait();
    GameLoadGui:Destroy();
    task.wait(10);
end;

