            end)
        game:GetService("UserInputService").WindowFocusReleased:connect(
            function()
                game.RunService:Set3dRenderingEnabled(false)
            end)
    end)
    page2:Toggle("Black Screen", false, function(value)
        _G.Settings.HUD["Black Screen"] = value
        if value then
