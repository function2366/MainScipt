    page6:Toggle("Auto Fruit Mastery", _G.Settings.Mastery["Auto Farm Fruit Mastery"], function(value)
        _G.Settings.Mastery["Auto Farm Fruit Mastery"] = value
        if value == false then
            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
        end
        SaveSettings()
    end)

    page6:Toggle("Auto Gun Mastery", _G.Settings.Mastery["Auto Farm Gun Mastery"], function(value)
        _G.Settings.Mastery["Auto Farm Gun Mastery"] = value
        if value == false then
            toTarget(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame)
        end
