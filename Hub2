        pcall(function()
            if input.KeyCode == Enum.KeyCode.RightControl then
                if library.toggledui == false then
                    library.toggledui = true
                    tween:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In),
                        { Size = UDim2.new(0, 0, 0, 0) }):Play()
                    wait(.3)
                    STRIKECourseScreen.Enabled = false
                else
                    library.toggledui = false
                    tween:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back),
                        { Size = UDim2.new(0, 550, 0, 514) })
                        :Play()
                    STRIKECourseScreen.Enabled = true
                end
            end
        end)
    end)

    local imageclickframe                  = Instance.new("ImageLabel")
