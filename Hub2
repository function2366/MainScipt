                        if BuyGodhuman ~= 1 and HasSuperhuman and HasTalon and HasKarate and HasDeathStep and HasElectricClaw then
                            if HasSuperhuman and not SupComplete then
                                local args = {
                                    [1] = "BuySuperhuman"
                                }
